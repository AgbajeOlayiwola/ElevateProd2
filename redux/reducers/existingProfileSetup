import { setupProfile, otp, sendCac, getCAC, existingBusnessSetup } from '../types/actionTypes';

const initialState = {
    cacName:'',
    cacNameError:'',
    getCacName:'',
    getCacNameError:'',
    existingProfileSetupPay:'',
    existingProfileSetupError:''


};

const ExistingProfileSetupReducer = (state = initialState, { type, payload }) => {
    switch (type) {
        case sendCac.SEND_CAC_START:
            return {
                ...state,
            };
        case sendCac.SEND_CAC_SUCCESS:
            return {
                ...state,
                cacName: payload
            };
            case sendCac.SEND_CAC_ERROR:
                return {
                    ...state,
                    cacNameError: payload
            };
            case getCAC.GET_CAC_START:
                return {
                    ...state,
                };
            case getCAC.GET_CAC_SUCCESS:
                return {
                    ...state,
                    getCacName: payload
                };
                case getCAC.GET_CAC_ERROR:
                    return {
                        ...state,
                        getCacNameError: payload
                };

                case existingBusnessSetup.EXISTING_BUSINESS_START:
                    return {
                        ...state,
                    };
                case existingBusnessSetup.EXISTING_BUSINESS_SUCCESS:
                    return {
                        ...state,
                        existingProfileSetupPay: payload
                    };
                    case existingBusnessSetup.EXISTING_BUSINESS_ERROR:
                        return {
                            ...state,
                            existingProfileSetupError: payload
                    };
           
       
        default:
            return state;
    }
};

export default ExistingProfileSetupReducer;
